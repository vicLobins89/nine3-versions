{
  "name": "nine3-versions",
  "version": "1.0.0",
  "description": "Adds ability to quickly clone page hierarchy trees and eases page sorting/versioning.",
  "main": "build/scripts.js",
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:scripts": "wp-scripts build js/scripts.js",
    "build:styles": "sass sass:",
    "build:postcss": "postcss -r styles.css",
    "watch": "run-p \"watch:*\"",
    "watch:scripts": "wp-scripts start js/scripts.js",
    "watch:styles": "sass --watch sass:"
  },
  "author": "93Devs",
  "license": "ISC",
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@wordpress/browserslist-config": "^3.0.1",
    "@wordpress/scripts": "^26.1.0",
    "cssnano": "^4.1.10",
    "postcss": "^8.1.0",
    "postcss-cli": "^8.3.1",
    "postcss-focus": "^5.0.1",
    "postcss-loader": "^4.0.3",
    "postcss-preset-env": "^7.0.0",
    "postcss-pxtorem": "^6.0.0",
    "sass": "^1.32.7",
    "stylelint": "^13.10.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-config-wordpress": "^17.0.0",
    "stylelint-order": "^4.1.0",
    "stylint": "^2.0.0"
  },
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
